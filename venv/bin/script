#!/home/bruno/MEGA/code/click_tests/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'package','console_scripts','script'
__requires__ = 'package'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('package', 'console_scripts', 'script')()
    )
