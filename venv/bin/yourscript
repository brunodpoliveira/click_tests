#!/home/bruno/MEGA/code/click_tests/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'testpackage','console_scripts','yourscript'
__requires__ = 'testpackage'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('testpackage', 'console_scripts', 'yourscript')()
    )
